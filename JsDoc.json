{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["app.js"],
      "exclude": ["node_modules"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "readme": "README.md"
    }
  }
  